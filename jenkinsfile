#! groovy

pipeline{
  agent any
  stages{
    stage("build"){
      steps{
        echo "building ..."
        echo "Running ${env.BUILD_ID} on ${env.JENKISN_URL}"
        sleep 5
      }
    }
    stage("test"){
      steps{
        echo "Testing..."
        echo "Running test job on ${env.NODE_NAME}"
      }
    }
    stage("deploy"){
      steps{
        echo "deploy..."
        echo "deploy branch is ${env.BRANCH_NAME}"
      }
    }
  }
}
