#! groovy

pipeline{
  agent any
  stages{
    stage("build"){
      steps{
        echo "building ..."
        sleep 5
      }
    }
    stage("test"){
      steps{
        echo "Testing..."
        echo "${env.BRANCH_NAME}"
      }
    }
    stage("deploy"){
      steps{
        echo "deploy..."
        echo "deploy branch is ${env.BRANCH_NAME}"
      }
    }
  }
}
